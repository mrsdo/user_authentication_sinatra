<body>
<div class="container">
  <!-- Navigation -->
  <header>
    <h4 class="logo">The R. Dolce Group</h4>
    <nav>
      <ul>
        <li><a href="/">HOME</a></li>
        <% if logged_in? %>
          <li><a href="/users/dashboard">VIEW PROFILE</a></li>
          <li><a href="/listings">MY LISTINGS</a></li>
          <li><a href="/logout">[ LOGOUT ]</a></li>
        <% else %>
          <li><a href="/listings">LISTINGS</a></li>
          <li><a href="/agents">AGENTS</a></li>
          <li><a href="/companies">COMPANIES</a></li>
          <li><a href="/login">LOGIN</a></li>
        <% end %>
      </ul>

    </nav>
  </header>
  <!-- Hero Section -->
  <section class="hero" id="hero"></section>
  <section>
    <div STYLE="height: 30px;background-color: #d85751; color: #ffffff; padding: 10px 5px 5px 5px;">
      <div style="float: left;">PLEASE NOTE:  This is a development site, not all features may work as expected.</div>
      <div style="float: right;margin-top: -15px;padding-right: 45px;"><% if logged_in? %>
        <h3>Welcome back, <%= current_user.fname %>!</h3>

      <% else %>

        <% end %>
      </div>

    </div>
    <br/>
    <br/>
    <!-- Start Dynamic content -->
    <div style="margin: 25px;">


      <%= yield %>
      <!-- End Dynamic content -->
    </div>
  </section>

<section class="banner">
  <h2 class="parallax">345 Carl Street Apt 12, Carrol Rd. Fort Myers, FL 33913</h2>
  <p class="parallax_description">Phone:: +1 239 555 1234 | Email: marti@rdolcegroup.com</p>
</section>
<section class="footer_banner" id="contact">
  <h2 class="hidden">Footer Banner Section </h2>
  <p class="hero_header">REGISTER YOUR PROPERTIES TODAY!</p>
  <div class="button"><a href="/signups">subscribe</a></div>
</section>
<!-- Copyrights Section -->
<div class="copyright">&copy;2021- <strong>ERG, All Rights Reserved. marti@rdolcegroup.com | +1 239 555 1234</strong>
</div>
</div>
</body>
</html>

